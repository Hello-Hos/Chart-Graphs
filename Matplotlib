import matplotlib.pyplot as plt
%matplotlib inline

cause = 'chronic','unintentional injury','alzimers','influenza and pneumonia','sepsis','others'
percentile = [45,10,15,5,6,9]

plt.figure(figsize=(6,6))
#explode=(0.05,0,0,0,0,0)
plt.pie(percentile,labels=cause,autopct='%1.1f%%',startangle= 70)
#plt.axis('equal')
plt.title('Disease leading to deaths report 2012')
plt.show()



